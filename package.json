{
  "name": "ayeful",
  "version": "999.999.999",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/ayeful"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/gl": "4.1.0",
    "@types/jasmine": "3.5.11",
    "@types/node": "13.13.14",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "del-cli": "3.0.1",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "gl": "4.5.3",
    "jasmine": "3.5.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "typescript": "4.0.2"
  },
  "scripts": {
    "buildClean": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\"",
    "buildTypeScript": "npm-run-all buildTypeScriptCore buildTypeScriptMat4UnitA buildTypeScriptMat4UnitB buildTypeScriptMat4UnitC buildTypeScriptMat4UnitD buildTypeScriptMat4UnitE buildTypeScriptMat4UnitF buildTypeScriptMat4UnitG buildTypeScriptMat4UnitH buildTypeScriptMat4UnitI buildTypeScriptMat4UnitJ buildTypeScriptMat4UnitK buildTypeScriptMat4UnitL buildTypeScriptMat4UnitM buildTypeScriptMat4UnitN buildTypeScriptMat4UnitO buildTypeScriptMat4UnitP buildTypeScriptMat4UnitQ buildTypeScriptMat4UnitR buildTypeScriptMat4UnitS buildTypeScriptMat4UnitT buildTypeScriptMat4UnitU",
    "buildTypeScriptCore": "tsc --project core.tsconfig.json",
    "buildTypeScriptMat4UnitA": "tsc --project mat4-unit-a.tsconfig.json",
    "buildTypeScriptMat4UnitB": "tsc --project mat4-unit-b.tsconfig.json",
    "buildTypeScriptMat4UnitC": "tsc --project mat4-unit-c.tsconfig.json",
    "buildTypeScriptMat4UnitD": "tsc --project mat4-unit-d.tsconfig.json",
    "buildTypeScriptMat4UnitE": "tsc --project mat4-unit-e.tsconfig.json",
    "buildTypeScriptMat4UnitF": "tsc --project mat4-unit-f.tsconfig.json",
    "buildTypeScriptMat4UnitG": "tsc --project mat4-unit-g.tsconfig.json",
    "buildTypeScriptMat4UnitH": "tsc --project mat4-unit-h.tsconfig.json",
    "buildTypeScriptMat4UnitI": "tsc --project mat4-unit-i.tsconfig.json",
    "buildTypeScriptMat4UnitJ": "tsc --project mat4-unit-j.tsconfig.json",
    "buildTypeScriptMat4UnitK": "tsc --project mat4-unit-k.tsconfig.json",
    "buildTypeScriptMat4UnitL": "tsc --project mat4-unit-l.tsconfig.json",
    "buildTypeScriptMat4UnitM": "tsc --project mat4-unit-m.tsconfig.json",
    "buildTypeScriptMat4UnitN": "tsc --project mat4-unit-n.tsconfig.json",
    "buildTypeScriptMat4UnitO": "tsc --project mat4-unit-o.tsconfig.json",
    "buildTypeScriptMat4UnitP": "tsc --project mat4-unit-p.tsconfig.json",
    "buildTypeScriptMat4UnitQ": "tsc --project mat4-unit-q.tsconfig.json",
    "buildTypeScriptMat4UnitR": "tsc --project mat4-unit-r.tsconfig.json",
    "buildTypeScriptMat4UnitS": "tsc --project mat4-unit-s.tsconfig.json",
    "buildTypeScriptMat4UnitT": "tsc --project mat4-unit-t.tsconfig.json",
    "buildTypeScriptMat4UnitU": "tsc --project mat4-unit-u.tsconfig.json",
    "buildJasmineUnit": "jasmine --config=unit.jasmine.json",
    "buildNyc": "nyc --check-coverage --all --exclude \"**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"**/unit.*\" --lines 100 --functions 100 --branches 100 --statements 100 npm run-script buildJasmineUnit",
    "test": "npm-run-all buildClean buildTypeScript buildNyc",
    "lint": "eslint \"**/*.ts\""
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.js",
    "!**/unit.d.ts"
  ]
}
